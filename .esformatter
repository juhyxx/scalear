{
	"plugins": [
		"esformatter-collapse-objects",
		"esformatter-braces",
		"esformatter-quotes",
		"esformatter-dot-notation",
		"esformatter-parseint",
		"esformatter-remove-trailing-commas",
		"esformatter-semicolons",
		"esformatter-parseint"

	],
	"indent": {
		"value": "\t",
		"FunctionExpression": 1,
		"ArrayExpression": 1,
		"ObjectExpression": 1
	},
	"collapseObjects": {
		"ObjectExpression": {
			"maxLineLength": 80,
			"maxKeys": 3,
			"maxDepth": 2,
			"forbidden": [
				"FunctionExpression"
			]
		},
		"ArrayExpression": {
			"maxLineLength": 80,
			"maxKeys": 3,
			"maxDepth": 2,
			"forbidden": [
				"FunctionExpression"
			]
		}
	},
	"quotes": {
		"type": "single",
		"avoidEscape": false
	}
}
